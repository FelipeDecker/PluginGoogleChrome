{
    "manifest_version": 2,
    "name": "Insta Folows",
    "version": "1.0",
    "description": "Seguidores para instagran",
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}